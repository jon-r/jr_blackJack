{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "rules": {
    "indent": [
            "error",
            2
        ],
    "linebreak-style": 0,
    "semi": [
            "error",
            "always"
        ],
    "no-irregular-whitespace": [
          "error"
        ],
    "no-console":0,
    "no-trailing-spaces": [
      "error",
      { "skipBlankLines": true }
    ]

  }
}
